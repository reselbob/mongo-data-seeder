{
  "name": "mongo-data-seeder",
  "version": "0.0.5",
  "description": "An application that automatically seeds a mongo database with random data",
  "main": "dist/src/index.js",
  "bin": {
    "seeder": "./dist/src/index.js"
  },
  "scripts": {
    "start": "tsc && node dist/src/index.js",
    "create": "npm run build && npm run test",
    "build": "tsc -p .",
    "dev": "ts-node src/index.ts",
    "prod": "npm install -g && ts-node src/index.ts",
    "test": "mocha -r ts-node/register tests/test.ts --exit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/reselbob/mongo-data-seeder.git"
  },
  "keywords": [
    "mongo"
  ],
  "author": "Bob Reselman <reselbob@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/reselbob/mongo-data-seeder/issues"
  },
  "homepage": "https://github.com/reselbob/mongo-data-seeder#readme",
  "devDependencies": {
    "@types/chai": "^4.2.14",
    "@types/express": "^4.17.13",
    "@types/faker": "^5.5.7",
    "@types/mocha": "^8.2.0",
    "@types/yargs": "^17.0.2",
    "chai": "^4.3.4",
    "dotenv": "^10.0.0",
    "mocha": "^9.0.3",
    "ts-node": "^10.1.0",
    "tslint": "^6.1.3",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "faker": "^5.5.3",
    "mongoose": "^5.13.5",
    "yargs": "^16.2.0"
  }
}
